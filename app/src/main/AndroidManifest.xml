<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yj.constraintlayout20">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".motion_layout.MotionLayoutActivity"></activity>
        <activity android:name=".gone_margin.GoneMarginActivity" />
        <activity android:name=".layer.LayerActivity" />
        <activity android:name=".layer.SelfLayerActivity" />
        <activity android:name=".barrier.BarrierActivity" />
        <activity android:name=".group.GroupActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>